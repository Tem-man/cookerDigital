<view>
  <xr-start
    disable-scroll
    id="main-frame"
    width="{{renderWidth}}"
    height="{{renderHeight}}"
    style="width:{{width}}px;height:{{height}}px;"
    bind:send="recieve"
    temperature="{{temperature}}"
    fog="{{fog}}"
    btnY="{{btnY}}"
    isShowParticle="{{isShowParticle}}"
    isStart="{{isStart}}"
   
  />
  <panel-ui bind:panelevent="panelControl"></panel-ui>
  
</view>